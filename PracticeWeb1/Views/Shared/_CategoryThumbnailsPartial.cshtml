@model PracticeWeb1.Entities.Category


    <div class="panelContainer">
        <div class="panel panel-primary catThumbnail">
            <div class="panel-heading">@Model.Title</div>
            <div class="panel-body">
                <a href="/Home/AllProducts/@Model.Id">
                    <img src="~/Images/Uploads/Categories/@Model.Id/Thumbs/@Model.ImageUrl" alt="@Model.ImageUrl" />
                </a>
            </div>
            <div class="panel-footer">@Model.Description</div>
        </div>
    </div>



